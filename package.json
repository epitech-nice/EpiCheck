        "@react-navigation/native-stack": "^7.5.1",
        "axios": "^1.12.2",
        "expo": "^54.0.20",
        "expo-audio": "~1.0.14",
        "expo-auth-session": "^7.0.8",
        "expo-camera": "^17.0.8",
        "expo-crypto": "^15.0.7",
        "expo-document-picker": "~14.0.7",
        "expo-secure-store": "^15.0.7",
        "expo-status-bar": "~3.0.8",
        "expo-web-browser": "^15.0.8",
        "moment": "^2.30.1",
        "nativewind": "^4.2.1",
