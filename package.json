        "@types/react": "~19.1.0",
        "babel-preset-expo": "^54.0.6",
        "eslint": "^9.0.0",
        "eslint-config-expo": "~10.0.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
