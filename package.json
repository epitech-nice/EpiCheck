{
    "name": "epicheck",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "start": "expo start",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "proxy": "cd proxy-server && node server.js",
        "build:android:debug": "expo run:android --variant debug",
        "build:android:release": "cd android && ./gradlew assembleRelease && cd ..",
        "build:apk": "cd android && ./gradlew assembleRelease && cd ..",
        "lint": "expo lint",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,yaml}\""
    },
    "dependencies": {
